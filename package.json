    "dependencies": {
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "cross-env": "^5.2.0"
    },
    "devDependencies": {
        "windwalker-fusion": "^0.1.9"
    }
